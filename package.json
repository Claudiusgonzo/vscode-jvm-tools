{
	"name": "jvm-tools",
	"displayName": "JVM Tools",
	"description": "(Preview) JVM Tools Accessible from Visual Studio Code",
	"version": "0.0.1",
	"publisher": "brunoborges",
	"license": "MIT",
	"preview": true,
	"private": true,
	"repository": {
		"url": "https://github.com/microsoft/vscode-jvm-tools",
		"type": "git"
	},
	"badges": [],
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"onView:jvmList"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "jvmList.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg.png",
					"dark": "resources/dark/refresh.svg.png"
				}
			},
			{
				"command": "jvmList.openJConsole",
				"title": "JConsole",
				"icon": {
					"light": "resources/light/edit.svg.png",
					"dark": "resources/dark/edit.svg.png"
				}
			}
		],
		"views": {
			"debug": [
				{
					"id": "jvmList",
					"name": "JVM List"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "jvmList.refresh",
					"when": "view == jvmList",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "jvmList.openJConsole",
					"when": "view == jvmList && viewItem == jvm",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
